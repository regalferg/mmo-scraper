    <div id="wrapper">
      <h1>Ha Ha No MMOs Here</h1>
      <h2><i>Learned Something f00!</i></h2>
      <div id="articles"></div>
          {{#each dbArticle}}
         <div class="card" id="post">
                     <div class="card-block">
                       <span id="post-body">{{title}}</span>><button type="button" class="btn btn-primary deletez" data-id="{{_id}}">Delete Article</button>
                       <button type="button"  class="btn btn-primary notez "data-id="{{_id}}" data-toggle="modal" data-target="#notesModal">Notes</button
                        <br>
                        <br>
                        <a href="#" id="postername">{{link}}</a><span id="says"> </span>
                        {{!-- <div id ="linky" style="display:none;" data-id="{{_id}}"></div> --}}
                         {{!-- <div id="notes"></div> --}}
                    </div>
                </div>
        {{/each}}
        <!-- Modal -->
<div class="modal fade" id="notesModal" tabindex="-1" role="dialog" aria-labelledby="notesModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="notesModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p id="notes">Test</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" id ="closeBtn" data-dismiss="modal" >Close</button>
        {{!-- <button type="button" class="btn btn-primary">Save changes</button> --}}
      </div>
    </div>
  </div>
</div>

      
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="/javascripts/appz.js"></script>
  </body>